{
    "name": "softspring/customer-bundle",
    "description": "This bundle provides customer features.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "softspring/crudl-bundle": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CustomerBundle\\": ""
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CustomerBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "suggest": {
        "softspring/platform-bundle": "Integrate external platforms",
        "softspring/stripe-platform": "Integrate stripe platform"
    },
    "require-dev": {
        "symfony/http-kernel": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "phpunit/phpunit": "^9.1",
        "doctrine/common": "^2.12",
        "softspring/doctrine-templates": "^1.1"
    }
}
